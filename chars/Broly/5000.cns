; HITA_UP_T (transition)
[Statedef 5035]
type    = A
movetype= H
physics = N


[State 5035, 1]
type = ChangeAnim
trigger1 = Time = 0
trigger1 = SelfAnimExist(5035)
trigger1 = Anim != [5051,5059] ;Not if in hit up anim
trigger1 = Anim != 5090 ;Not if hit off ground anim
value = 5035

[State 5035, GetHitVar(fall.recovertime)=2000]
type = ChangeAnim
trigger1 =GetHitVar(fall.recovertime)=2000
trigger1 = Time = 0
trigger1 = SelfAnimExist(5035)
trigger1 = Anim != [5051,5059] ;Not if in hit up anim
trigger1 = Anim != 5090 ;Not if hit off ground anim
value = 5035


;-Con este ctrl llamamos a la animación de transición
;-al caer, de los sprites de quema kof amarilla.
[State 5035,Quema amarilla kof]
type = ChangeAnim
trigger1 =GetHitVar(fall.recovertime)=20000;
trigger1 = Time = 0
trigger1 = SelfAnimExist(20035)
trigger1 = Anim != [20051,20059] ;Not if in hit up anim
trigger1 = Anim != 20090 ;Not if hit off ground anim
value = 20035


;-Con este ctrl llamamos a la animación de transición
;-al caer, de los sprites de quema kof morada.
[State 5035,Quema morada kof]
type = ChangeAnim
trigger1 =GetHitVar(fall.recovertime)=22000;
trigger1 = Time = 0
trigger1 = SelfAnimExist(22035)
trigger1 = Anim != [22051,22059] ;Not if in hit up anim
trigger1 = Anim != 22090 ;Not if hit off ground anim
value = 22035


[State 5035, 2]
type = VelAdd
trigger1 = 1
y = GetHitVar(yaccel)


[State 5035, 3]
type = ChangeState
triggerALL =GetHitVar(fall.recovertime)!=2000
triggerALL =GetHitVar(fall.recovertime)!=20000
triggerALL =GetHitVar(fall.recovertime)!=22000
triggerall = !HitFall
trigger1 = HitOver  ;Hit is over
trigger2 = AnimTime = 0 ;Animation over
trigger3 = Vel Y > 0    ;Hit ground
trigger3 = Pos Y >= 10  ;.
trigger4 = Time = 0     ;No transition anim
trigger4 = Anim != 5035 ;.
value = 5040 ;HITA_RECOV



[State 5035, 4]
type = ChangeState
triggerall = HitFall
trigger1 = HitOver  ;Hit is over
trigger2 = AnimTime = 0 ;Anim is over
trigger3 = Vel Y > 0    ;Hit ground
trigger3 = Pos Y >= 10  ;.
trigger4 = Time = 0     ;No transition anim
trigger4 = Anim != 5035 ;.
value = 5050 ;HITA_FALL



;-ctrl que cambia a la caída especial estando en las
;- gethitvar(fall.recovertime)=20000,22000 y 2000 fall=0,importante
;-no quitar,sino no pasa al estado 5050,con fall=1 se
;-pasa con el ctrl normal de arriba.
[State 5035, 5]
type = ChangeState
triggerall = HitShakeOver
triggerall = Vel Y < 0    ;Hit ground
triggerall = Pos Y = 0  ;.
trigger1 = gethitvar(fall.recovertime)=2000  && HitFall =0
trigger2 = gethitvar(fall.recovertime)=20000  && HitFall =0
trigger3 = gethitvar(fall.recovertime)=22000  && HitFall =0
;trigger1 = Time = 0     ;No transition anim
value = 5050 ;HITA_FALL

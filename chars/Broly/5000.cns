;-----------------------------------------



[State 5000, 1] ;Anim for HIT_LIGHT to HIT_HARD
type = ChangeAnim
triggerall = GetHitVar(animtype) != [3,5]
trigger1 = Time = 0
trigger2 =HitShakeOver && (GetHitVar(fall.recovertime)=20000)+ifelse((GetHitVar(groundtype) = 1),5000,5010)
trigger3 =HitShakeOver && (GetHitVar(fall.recovertime)=22000)+ifelse((GetHitVar(groundtype) = 1),5000,5010); && GetHitVar(groundtype) =[0,2];-sirve para frizzear la animación.
value = ifelse((GetHitVar(groundtype) = 1),5000,5010) + GetHitVar(animtype) + ifelse((GetHitVar(fall.recovertime)=20000),14000,0) + ifelse((GetHitVar(fall.recovertime)=22000),16000,0)+ ifelse(HitShakeOver,1000,0);+ ifelse((GetHitVar(fall.recovertime)=20000)&&HitShakeOver,1000,0)


[State 5000, 2] ;Anim for HIT_BACK
type = ChangeAnim
trigger1 =  (loseko! ) &&  (prevstateno!= [120,153 ]);-Ayuda al sistema colapso k.o
trigger1 = Time=0
trigger1 = GetHitVar(animtype) = [3,5]
value = 5030 + ifelse((GetHitVar(fall.recovertime)=20000),15000,0) + ifelse((GetHitVar(fall.recovertime)=22000),17000,0)


[State 5000, 2] ;Anim for HIT_BACK
type = ChangeAnim
trigger1 =Time =1 ;-Ayuda al sistema colapso k.o
trigger1 = GetHitVar(animtype) = [3,5]
value = 5030 + ifelse((GetHitVar(fall.recovertime)=20000),15000,0) + ifelse((GetHitVar(fall.recovertime)=22000),17000,0)


[State 5000, 3] ;Anim for HIT_UP/HIT_DIAGUP (only if it exists)
type = ChangeAnim
trigger1 =  (loseko! ) &&  (prevstateno!= [120,153 ]);-Ayuda al sistema colapso k.o
trigger1 = Time=0
trigger1 = (GetHitVar(animtype) = [4,5]) && (SelfAnimExist(5047 + GetHitVar(animtype)))
value = 5047 + GetHitVar(animtype) + ifelse((GetHitVar(fall.recovertime)=20000),15000,0)+ ifelse((GetHitVar(fall.recovertime)=22000),17000,0)


[State 5000, 3] ;Anim for HIT_UP/HIT_DIAGUP (only if it exists)
type = ChangeAnim
trigger1 =Time =1 ;-Ayuda al sistema colapso k.o
trigger1 = (GetHitVar(animtype) = [4,5]) && (SelfAnimExist(5047 + GetHitVar(animtype)))
value = 5047 + GetHitVar(animtype) + ifelse((GetHitVar(fall.recovertime)=20000),15000,0)+ ifelse((GetHitVar(fall.recovertime)=22000),17000,0)


[State 5000, 4] ;Freeze anim
type = ChangeAnim
triggerALL =GetHitVar(fall.recovertime)!= 20000
triggerALL =GetHitVar(fall.recovertime)!= 22000
trigger1 = Time > 0
value = anim


[State 5000, 5] ;State type gets set to aerial if getting hit up
type = StateTypeSet
trigger1 = Time = 0
trigger1 = GetHitVar(yvel) != 0 && GetHitVar(fall)
trigger2 = Pos Y != 0
type = a


[State 5000, 6]
type = ChangeState
trigger1 = HitShakeOver
trigger1 = GetHitVar(yvel) = 0
value = 5001 ;HITG_SLIDE


[State 5000, 6]
type = ChangeState
triggerall = HitShakeOver
triggerall = GetHitVar(yvel) = 0
trigger1 =GetHitVar(fall.recovertime)= 20000
trigger2 =GetHitVar(fall.recovertime)= 22000
value = 5001 ;HITG_SLIDE




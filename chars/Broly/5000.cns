; HITA_UP (initial going up)
[Statedef 5030]
type    = A
movetype= H
physics = N
ctrl = 0


;----------------------------------------
;-Explods que anuncia los carteles de recobrarse
;-en tierra,solo puede en un estado de caída el personaje
;-recobrase .

;-Carteles anunciando recobrarse p1.
[State 5030, recovery P1] ; "recovery !" sprite
type = Explod
triggerall = numexplod(7260) <= 0 && teamside = 1
triggerall = Time =0
triggerall = alive!=0
trigger1 = GetHitVar(fall) =1
trigger2 = gethitvar(fall.recovertime)=2000
trigger3 = gethitvar(fall.recovertime)=20000
trigger4 = gethitvar(fall.recovertime)=22000
anim = 7260
id   = 7260
pos =-320,50
postype = right
bindtime = -1
supermove = 1
facing = 1
ontop = 1
ownpal = 1
ignorehitpause = 1
persistent = 0


;-Carteles anunciando recobrarse p2.
[State 5030, recovery P2] ; "recovery !" sprite
type = Explod
triggerall = numexplod(7261) <= 0 && teamside = 2
triggerall = Time =0
triggerall = alive!=0
trigger1 = GetHitVar(fall) =1
trigger2 = gethitvar(fall.recovertime)=2000
trigger3 = gethitvar(fall.recovertime)=20000
trigger4 = gethitvar(fall.recovertime)=22000
anim = 7261
id   = 7261
pos =231,50
postype = left
bindtime = -1
supermove = 1
facing = 1
ontop = 1
ownpal = 1
ignorehitpause = 1
persistent = 0

;----------------------------------------

[State 5030, 1]
type = VelAdd;Gravity
trigger1 = 1
y = GetHitVar(yaccel)

[State 5030, 2]
type = HitVelSet
trigger1 = Time = 0
x = 1
y = 1

[State 5030, 3]
type = ChangeState
triggerall = gethitvar(fall.recovertime)!=2000
triggerall = gethitvar(fall.recovertime)!=20000
triggerall = gethitvar(fall.recovertime)!=22000
triggerall = !HitFall
trigger1 = HitOver
trigger2 = Vel Y > 0    ;Hit ground
trigger2 = Pos Y >= 10  ;.
value = 5040 ;HITA_RECOV


[State 5030, 4]
type = ChangeState
triggerall = HitFall
trigger1 = HitOver  && HitShakeOver
trigger2 = Vel Y > 0    ;Hit ground
trigger2 = Pos Y >= 10  ;.
value = 5050 ;HITA_FALL


;Ctrl para caída en sprites de quema y también
;-para la caída del  fall.recovertime=2000.
[State 5030, 5]
type = ChangeState
triggerall = Vel Y < 0    ;Hit ground
triggerall = Pos Y = 0  ;.
trigger1 = gethitvar(fall.recovertime)=2000 && HitFall =0
trigger2 = gethitvar(fall.recovertime)=20000 && HitFall =0
trigger3 = gethitvar(fall.recovertime)=22000 && HitFall =0
value = 5035 ;HITA_FALL



[State 5030, 6]
type = ChangeState
trigger1 = AnimTime = 0
value = 5035 ;HITA_UP (transition)



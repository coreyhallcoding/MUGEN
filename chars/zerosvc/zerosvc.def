st3	= zerosvc-h.cns	;Hyper Moves
st4	= override.cns	;Override States
st5	= helpers.cns	;Helper States
st6	= cybr-elf.cns	;Cyber-Elf States
st7	= zerosvc.def	;Misc. States
;--------------------
;[Palettes] (can have up to 12)
;--------------------
pal1	= zero.act
pal2	= zero2.act
pal3	= zero3.act
pal4	= zero4.act
pal5	= zero5.act
pal6	= zero6.act
pal7	= matrix.act
pal8	= matrix.act
pal9	= matrix.act
pal10	= silver.act
pal11	= silver.act
